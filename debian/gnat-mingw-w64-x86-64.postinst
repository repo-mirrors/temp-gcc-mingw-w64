#! /bin/sh
# postinst script for gnat-mingw-w64-x86-64
#
# see: dh_installdeb(1)

set -e

# summary of how this script can be called:
#        * <postinst> `configure' <most-recently-configured-version>
#        * <old-postinst> `abort-upgrade' <new version>
#        * <conflictor's-postinst> `abort-remove' `in-favour' <package>
#          <new-version>
#        * <deconfigured's-postinst> `abort-deconfigure' `in-favour'
#          <failed-install-package> <version> `removing'
#          <conflicting-package> <version>
# for details, see http://www.debian.org/doc/debian-policy/ or
# the debian-policy package
#

case "$1" in
    configure)

        update-alternatives --install /usr/bin/x86_64-w64-mingw32-gnat x86_64-w64-mingw32-gnat /usr/bin/x86_64-w64-mingw32-gnat-posix 30 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatbind x86_64-w64-mingw32-gnatbind /usr/bin/x86_64-w64-mingw32-gnatbind-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatchop x86_64-w64-mingw32-gnatchop /usr/bin/x86_64-w64-mingw32-gnatchop-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatclean x86_64-w64-mingw32-gnatclean /usr/bin/x86_64-w64-mingw32-gnatclean-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatfind x86_64-w64-mingw32-gnatfind /usr/bin/x86_64-w64-mingw32-gnatfind-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatkr x86_64-w64-mingw32-gnatkr /usr/bin/x86_64-w64-mingw32-gnatkr-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatlink x86_64-w64-mingw32-gnatlink /usr/bin/x86_64-w64-mingw32-gnatlink-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatls x86_64-w64-mingw32-gnatls /usr/bin/x86_64-w64-mingw32-gnatls-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatmake x86_64-w64-mingw32-gnatmake /usr/bin/x86_64-w64-mingw32-gnatmake-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatname x86_64-w64-mingw32-gnatname /usr/bin/x86_64-w64-mingw32-gnatname-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatprep x86_64-w64-mingw32-gnatprep /usr/bin/x86_64-w64-mingw32-gnatprep-posix \
         --slave /usr/bin/x86_64-w64-mingw32-gnatxref x86_64-w64-mingw32-gnatxref /usr/bin/x86_64-w64-mingw32-gnatxref-posix

        update-alternatives --install /usr/bin/x86_64-w64-mingw32-gnat x86_64-w64-mingw32-gnat /usr/bin/x86_64-w64-mingw32-gnat-win32 60 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatbind x86_64-w64-mingw32-gnatbind /usr/bin/x86_64-w64-mingw32-gnatbind-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatchop x86_64-w64-mingw32-gnatchop /usr/bin/x86_64-w64-mingw32-gnatchop-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatclean x86_64-w64-mingw32-gnatclean /usr/bin/x86_64-w64-mingw32-gnatclean-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatfind x86_64-w64-mingw32-gnatfind /usr/bin/x86_64-w64-mingw32-gnatfind-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatkr x86_64-w64-mingw32-gnatkr /usr/bin/x86_64-w64-mingw32-gnatkr-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatlink x86_64-w64-mingw32-gnatlink /usr/bin/x86_64-w64-mingw32-gnatlink-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatls x86_64-w64-mingw32-gnatls /usr/bin/x86_64-w64-mingw32-gnatls-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatmake x86_64-w64-mingw32-gnatmake /usr/bin/x86_64-w64-mingw32-gnatmake-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatname x86_64-w64-mingw32-gnatname /usr/bin/x86_64-w64-mingw32-gnatname-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatprep x86_64-w64-mingw32-gnatprep /usr/bin/x86_64-w64-mingw32-gnatprep-win32 \
         --slave /usr/bin/x86_64-w64-mingw32-gnatxref x86_64-w64-mingw32-gnatxref /usr/bin/x86_64-w64-mingw32-gnatxref-win32

    ;;

    abort-upgrade|abort-remove|abort-deconfigure)

    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
